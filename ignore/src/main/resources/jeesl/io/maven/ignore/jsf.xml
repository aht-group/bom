<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0
							http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
<rules>
	<rule groupId="org.primefaces" comparisonMethod="maven">
      <ignoreVersions>
      	<ignoreVersion>8.0.RC1</ignoreVersion>
        <ignoreVersion>8.0.RC2</ignoreVersion>
      </ignoreVersions>
    </rule>
    <rule groupId="org.omnifaces" comparisonMethod="maven">
      <ignoreVersions>
      	<ignoreVersion>4.0-M1</ignoreVersion>
      	<ignoreVersion>4.0-M2</ignoreVersion>
      	<ignoreVersion>4.0-M3</ignoreVersion>
      	<ignoreVersion>4.0-M4</ignoreVersion>
      	<ignoreVersion>4.0-M5</ignoreVersion>
      	<ignoreVersion>4.0-M6</ignoreVersion>
      	<ignoreVersion>4.0-M7</ignoreVersion>
      	<ignoreVersion>4.0-M8</ignoreVersion>
      	<ignoreVersion>4.0-M9</ignoreVersion>
      	<ignoreVersion>4.0-M10</ignoreVersion>
      	<ignoreVersion>4.0-M11</ignoreVersion>
      	<ignoreVersion>4.0-M12</ignoreVersion>
      	<ignoreVersion>4.0-M13</ignoreVersion>
      	<ignoreVersion>4.0-M14</ignoreVersion>
      </ignoreVersions>
    </rule>
	<rule groupId="org.ocpsoft.rewrite" comparisonMethod="maven">
		<ignoreVersions>
			<ignoreVersion>4.0.0.Alpha1</ignoreVersion>
			<ignoreVersion>5.0.0.Alpha1</ignoreVersion>
			<ignoreVersion>6.0.0.Alpha1</ignoreVersion>
		</ignoreVersions>
	</rule>
	<rule groupId="commons-fileupload" comparisonMethod="maven">
      <ignoreVersions>
      	<ignoreVersion>1.5.0.redhat-00001</ignoreVersion>
      </ignoreVersions>
    </rule>
</rules>
</ruleset>